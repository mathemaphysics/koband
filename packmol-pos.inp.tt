
tolerance 2.0
output [% GET pdbout %]
filetype pdb
structure [% GET pdba %]
  resnumbers 3
  number [% GET na %]
  inside box 0 0 0 [% dx * (1.0-padf) * 10.0 %] [% dx * (1.0-padf) * 10.0 %] [% dx * (1.0-padf) * 10.0 %]
end structure
structure [% GET pdbb %]
  resnumbers 3
  number [% GET nb %]
  inside box 0 0 0 [% dx * (1.0-padf) * 10.0 %] [% dx * (1.0-padf) * 10.0 %] [% dx * (1.0-padf) * 10.0 %]
end structure
